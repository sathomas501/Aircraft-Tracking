#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/color-support/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules:/mnt/c/Users/satho/Documents/Projects/node_modules:/mnt/c/Users/satho/Documents/node_modules:/mnt/c/Users/satho/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/color-support/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules:/mnt/c/Users/satho/Documents/Projects/node_modules:/mnt/c/Users/satho/Documents/node_modules:/mnt/c/Users/satho/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../color-support/bin.js" "$@"
else
  exec node  "$basedir/../color-support/bin.js" "$@"
fi
