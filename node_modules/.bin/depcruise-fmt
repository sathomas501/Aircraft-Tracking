#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/dependency-cruiser@16.9.0/node_modules/dependency-cruiser/bin/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/dependency-cruiser@16.9.0/node_modules/dependency-cruiser/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/dependency-cruiser@16.9.0/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/dependency-cruiser@16.9.0/node_modules/dependency-cruiser/bin/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/dependency-cruiser@16.9.0/node_modules/dependency-cruiser/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/dependency-cruiser@16.9.0/node_modules:/mnt/c/Users/satho/Documents/Projects/Aircraft-Tracking/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dependency-cruiser/bin/depcruise-fmt.mjs" "$@"
else
  exec node  "$basedir/../dependency-cruiser/bin/depcruise-fmt.mjs" "$@"
fi
